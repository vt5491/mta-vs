{
  "name": "mta-vs",
  "displayName": "mta-vs",
  "description": "muti-theme applicator for vscode",
  "version": "0.0.1",
  "publisher": "vt5491",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mta-vs.sayHello"
  ],
  "main": "./out/src/mta-vs",
  "contributes": {
    "commands": [
      {
        "command": "mta-vs.sayHello",
        "title": "Hello World"
      }
